
<%= form_for @post do |f| %>
<%= f.select :name, City.order(:name),:id, :name, include_blank: true %><br>
<%= f.text_field :title, placeholder: "title" %> <br>
<%= f.text_field :author, placeholder: "author" %><br>
<%= f.text_field :body, placeholder: "body" %>
<hr>
<%= f.submit "Update" %>
<% end %>